web: bin/rails server -p 3000
tailwind: bin/rails tailwindcss:watch
sass: sass --watch ./app/_assets/application.scss:./app/assets/stylesheets/application.sass.css --sourcemap=inline
browser-sync: browser-sync start --port 4000 --ui-port 4001 --proxy localhost:3000 --no-open --no-notify --watch --files ./app
